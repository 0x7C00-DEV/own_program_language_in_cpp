def bubble_sort(arr: [int]) -> [int] {
    let n: int = Length(arr);
    for (i: int = 0; i < n - 1; ++i) {
        for (j: int = 0; j < n - 1 - i; ++j) {
            if (arr[j] > arr[j + 1]) {
                let temp: int = arr[j];
                arr[j] = arr[j + 1];
                arr[j + 1] = temp;
            }
        }
    }
    return arr;
}

let arr: [int] = [64, 34, 25, 12, 22, 11, 90];
Println("Original array:");
Println(arr);

let sorted: [int] = bubble_sort(arr);
Println("Sorted array:");
Println(sorted);